  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "generate": "openapi-generator-cli generate",
    "generate:build": "npm run generate && npm run build",
    "build": "sh ./scripts/prepair_openapi.sh && tsc",
