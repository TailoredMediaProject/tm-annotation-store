    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -R -f dist node_modules src/openapi package-lock.json scripts/init-kafka/package-lock.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@redlink/tm-annotation-store-api": "^2.0.34",
    "apollo-datasource": "^0.8.0",
    "apollo-server": "^2.23.0",
    "axios": "^0.26.1",
    "crypto": "^1.0.1",
    "graphql": "^15.5.0",
    "graphql-import-node": "0.0.4",
    "hash-sum": "^2.0.0",
    "kafkajs": "^1.15.0",
    "mongodb": "^4.4.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "rx-store": "^1.0.0",
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.13.13",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@types/hash-sum": "^1.0.0",
    "@types/morgan": "^1.9.3",
    "copy-node-modules": "^1.1.1",
    "nodemon": "^2.0.15",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
